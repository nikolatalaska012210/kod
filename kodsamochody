audi = {"przebieg": 120000, "rocznik": 2019, "automat": True}
volkswagen = {"przebieg": 198100, "rocznik": 2015, "automat": False}
fiat = {"przebieg": 90000, "rocznik": 2022, "automat": False}


Samochody = {"audi": audi, "volkswagen": volkswagen, "fiat": fiat}



nazwa_samochodu = [audi, volkswagen, fiat]
def wyszukaj_samochod(nazwa_samochodu):
    if nazwa_samochodu in Samochody:
        print(Samochody[nazwa_samochodu])
    else:
        print("Nie istnieje taki samochód w katalogu.")

nazwa_samochodu = input("Podaj nazwę samochodu: ")
wyszukaj_samochod(nazwa_samochodu)
